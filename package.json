{
  "name": "minsik",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.30.2",
    "react-scripts": "5.0.1",
    "source-map-explorer": "^2.5.3",
    "web-vitals": "^2.1.4",
    "zustand": "^4.3.7"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-sitemap": "node src/SEO/generateSitemap.js",
    "postbuild": "npm run generate-sitemap && PUPPETEER_ARGS='--no-sandbox --disable-setuid-sandbox' react-hydratable"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactHydratable": {
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"],
    "timeout": 30000
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "babel-plugin-named-exports-order": "^0.0.2",
    "postcss": "^8.5.6",
    "prop-types": "^15.8.1",
    "react-hydratable": "^1.5.0",
    "tailwindcss": "^3.4.19",
    "webpack": "^5.80.0"
  }
}
